// shared_lib.mpc
    
project (MathLibrary) {
    sharedname = MathLibrary     // Declare project as shared library
      
    dynamicflags += MY_SHARED_LIB_BUILD_DLL
      
    dllout = .                     // Define location of generated shared library
    libout = .                     // Define location of import library (Windows only)
      
    Source_Files {
		MathLibrary.cpp
    }
}